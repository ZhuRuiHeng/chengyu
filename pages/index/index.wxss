/**index.wxss**/
page{
  background: url(https://qncdn.playonwechat.com/kantucaici/home_bg.png);
  background-size:100% 100%;
}

.img_border{
  background: url(https://qncdn.playonwechat.com/kantucaici/img_box_bg.png);
  width: 397rpx;
  height: 392rpx;
  background-size: 100% 100%;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: 100rpx;
  z-index: -1;
}

.img_box{
  /* background: url(https://qncdn.playonwechat.com/kantucaici/img_box_bg.png); */
  width: 357rpx;
  height: 352rpx;
  background-size: 100% 100%;
  margin:0 auto;
  margin-top: 125rpx;
  display: flex;
  flex-wrap: wrap;
  overflow: hidden;
  border-radius: 20rpx;
   margin-bottom: 70rpx; 
}

.img_box view{
  width: 50%;
  height: 186rpx;
}

.answer{
  display: flex;
  justify-content: center;
  position: relative;
  /* margin-top: -60rpx; */
}

.answer view{
  width: 70rpx;
  height: 70rpx;
  margin:0 5px;
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
}

.font_box{
  margin-top: 100rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}

.font_box view{
   width: 80rpx;
   height: 77rpx;
   border:2px solid transparent;
   box-sizing: content-box;
}

.font_in view{
   width: 80rpx;
   height: 77rpx;
}

.font_box .has_houxuan{
   width: 80rpx;
   height: 77rpx;
   line-height: 77rpx;
   font-size: 36rpx;
   background-repeat: no-repeat;
   background-position: center;
   background-image: url('data:image/png;base64,');
   background-size: 99% 99%;
   text-align: center;
}

.answer .has_text{
  background:url('data:image/png;base64,');
  background-size: 99% 99%;
  text-align: center;
  line-height: 77rpx;
  background-repeat: no-repeat;
  background-position: center;
}

.answer_right_box{
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.5);
  left: 0;
  top: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 1000;
}

.answer_in{
  width: 585rpx;
  height: 767rpx;
  background: url('https://qncdn.playonwechat.com/kantucaici/answer_right_bg.png');
  background-size: 100% 100%;
  position: relative;
}

.answer .error{
  animation: error .1s infinite; 
}

@keyframes error{
  0%,100%{
    color: #333;
  }
  50%{
    color: #f7eab9;
  }
}

.nav_top{
  height: 116rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.pingyin{
  text-align: center;
  color: #f7eab9;
  font-weight: lighter;
  font-size: 30rpx;
}

.chengyu{
  text-align: center;
  letter-spacing: 20rpx;
  color: #f7eab9;
  font-size: 34rpx;
}

.jieshi{
  display: flex;
  padding:20rpx 30rpx;
  color: #412913;
}

.jieshi view:nth-of-type(1){
  font-size: 30rpx;
  width: 50prx;
  flex-shrink: 0;
  margin-right: 10rpx;
  color: #412913;
}

.jieshi view:nth-of-type(2){
  font-size: 30rpx;
  color: #412913;
}

.next_btn{
  width: 144rpx;
  height: 61rpx;
  background: url('https://qncdn.playonwechat.com/kantucaici/next_btn_bg.png');
  background-size: 100% 100%;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: 150rpx;
}

.tip{
  font-size: 30rpx;
  color: #412913;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: 36rpx;
  width: 100%;
  text-align: center;
}

.share{
  width: 116rpx;
  height: 59rpx;
  background: url('data:image/png;base64,');
  background-size: 100% 100%;
  position: fixed;
  right: 40rpx;
  top: 300rpx;
}
